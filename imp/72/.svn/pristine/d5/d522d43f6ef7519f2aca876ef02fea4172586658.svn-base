#include <stdio.h>
#include <stdlib.h>

int main(){
	 float Betrag;

	while(1){
		printf("Bitte geben Sie einen Betrag in Euro ein (Abbruch mit 0): ");
		scanf("%f", &Betrag);
		if(Betrag == 0){
			break;
		}
		else{
			printf("Der Betrag entspricht : %.3f \n", Betrag * 1.10);
		}

	}	


	return 0;
}