int contained(double x, double a, double b){

	if(x>a&&x<b) return 1;
	else return 0;
}

int main(){
	double a,b,x;
	printf("geben sie die intervallgrenzen a und b an\n");
	scanf("%lf %lf",&a,&b);
	printf("und jetzt noch x\n");
	scanf("%lf",&x);
	contained(x,a,b);
return 0;
}
