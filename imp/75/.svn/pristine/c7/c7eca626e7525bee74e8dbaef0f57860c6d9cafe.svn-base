int contained(double a, double b, double c) {
	return (a >= b && a <= c) || (a >= c && a <= b);
}

int main() {
	double a, b, c;
	printf("A? B? C?\n");
	scanf("%lf %lf %lf", &a, &b, &c);
	printf("%d", contained(a,b,c));
	return 0;
}
